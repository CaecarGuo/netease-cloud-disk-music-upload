{
    "manifest_version": 2,
    "name": "网易云音乐云盘管理",
    "description": "网易云音乐云盘管理，支持本地音乐上传至网易云盘",
    "version": "1.8",
    "author": "darren",
    "icons": {
        "32": "imgs/icon.png",
        "48": "imgs/icon.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "*://*.music.163.com/*",
        "cookies"
    ]
}